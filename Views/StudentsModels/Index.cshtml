@model IEnumerable<College.Areas.Identity.Data.StudentsModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>

@if (Model==null)
{
<p>Adam check</p>
}
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SchoolName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Percentage12th)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Percentage10th)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.City)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.isFeePaid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.YearOfStudy)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Attendance)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CGPA)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ImageURL)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
    @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SchoolName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Percentage12th)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Percentage10th)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.City)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.isFeePaid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.YearOfStudy)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Attendance)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CGPA)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ImageURL)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.RegNo">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.RegNo">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.RegNo">Delete</a>
                </td>
            </tr>
    }
        </tbody>
    </table>
